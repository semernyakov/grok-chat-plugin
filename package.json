{
  "name": "groq-chat-plugin",
  "version": "1.0.0",
  "description": "Obsidian plugin for Groq AI Chat",
  "main": "dist/main.js",
  "scripts": {
    "dev": "rollup -c -w",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.js production",
    "build-dev": "node esbuild.js",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "publish": "npm run build && npm run test && npm run release"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "groq",
    "ai",
    "chat"
  ],
  "author": "Semernyakov",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/jest": "^28.1.3",
    "@types/node": "^17.0.5",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "esbuild": "0.17.3",
    "esbuild-plugin-copy": "^2.1.1",
    "eslint": "^8.18.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "jest": "^28.1.1",
    "rollup": "^2.75.6",
    "ts-jest": "^28.0.5",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "obsidian": "^1.2.8",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
